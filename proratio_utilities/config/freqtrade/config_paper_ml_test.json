{
    "max_open_trades": 2,
    "stake_currency": "USDT",
    "stake_amount": 100,
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 10000,
    "cancel_open_orders_on_exit": false,

    "trading_mode": "spot",
    "margin_mode": "",

    "unfilledtimeout": {
        "entry": 10,
        "exit": 30
    },

    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },

    "exit_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1
    },

    "exchange": {
        "name": "binance",
        "key": "",
        "secret": "",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "BTC/USDT"
        ],
        "pair_blacklist": []
    },

    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],

    "edge": {
        "enabled": false
    },

    "telegram": {
        "enabled": false,
        "token": "dummy-token-telegram-disabled",
        "chat_id": "0"
    },

    "api_server": {
        "enabled": true,
        "listen_ip_address": "127.0.0.1",
        "listen_port": 8080,
        "verbosity": "info",
        "enable_openapi": true,
        "jwt_secret_key": "paper-trading-test-secret-key-change-in-production",
        "ws_token": "paper-trading-ws-token",
        "CORS_origins": ["http://localhost:3000"],
        "username": "admin",
        "password": "admin"
    },

    "bot_name": "ML_Paper_Trading_Test",
    "initial_state": "running",
    "force_entry_enable": true,

    "internals": {
        "process_throttle_secs": 5,
        "heartbeat_interval": 60,
        "sd_notify": false
    },

    "dataformat_ohlcv": "json",
    "dataformat_trades": "jsongz",

    "logfile": "user_data/logs/paper_trading_ml_test.log",

    "user_data_dir": "user_data",
    "datadir": "user_data/data/binance"
}
